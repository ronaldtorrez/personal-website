{
	"name": "personal-website",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"env:dev": "doppler run -p personal-website -c dev",
		"env:prod": "doppler run -p personal-website -c prd",
		"dev": "astro dev",
		"build": "astro build",
		"dev:env": "pnpm env:dev -- astro dev",
		"build:env": "pnpm env:prod -- astro build",
		"preview": "astro preview",
		"astro": "astro",
		"prepare": "husky",
		"lint": "eslint --ext .astro,.js,.ts,.css --fix"
	},
	"dependencies": {
		"@astrojs/mdx": "^4.3.12",
		"@astrojs/sitemap": "^3.6.0",
		"@playform/compress": "^0.2.0",
		"@tailwindcss/vite": "^4.1.14",
		"@vercel/speed-insights": "^1.2.0",
		"astro": "^5.16.0",
		"astro-cloudinary": "^1.3.5",
		"astro-llms-generate": "^1.3.3",
		"astro-seo": "^0.8.4",
		"axios": "^1.13.1",
		"phosphor-astro": "^2.1.0",
		"sharp": "^0.34.4",
		"slugify": "^1.6.6",
		"striptags": "^3.2.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.14"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@eslint/compat": "^2.0.0",
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.37.0",
		"@eslint/json": "^0.14.0",
		"@eslint/markdown": "^7.4.0",
		"@typescript-eslint/parser": "^8.46.1",
		"astro-eslint-parser": "^1.2.2",
		"eslint": "^9.37.0",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"jiti": "^2.6.1",
		"lint-staged": "^16.2.4",
		"prettier": "3.6.2",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-organize-attributes": "^1.0.0",
		"prettier-plugin-organize-imports": "^4.3.0",
		"prettier-plugin-sort-imports": "^1.8.8",
		"prettier-plugin-tailwindcss": "0.7.1",
		"tailwind-csstree": "^0.1.4",
		"typescript-eslint": "^8.46.1"
	},
	"lint-staged": {
		"**/*.{js,css,md,astro}": [
			"pnpm lint",
			"prettier --write"
